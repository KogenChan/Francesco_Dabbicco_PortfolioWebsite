{
   "routes": [
      {
         "src": "/robots.txt",
         "dest": "/robots.txt"
      },
      {
         "src": "/sitemap.xml",
         "dest": "/sitemap.xml"
      },
      {
         "src": "/(.*)",
         "dest": "/"
      }
   ],
   "headers": [
      {
         "source": "/robots.txt",
         "headers": [
            {
               "key": "Content-Type",
               "value": "text/plain"
            }
         ]
      }
   ]
}